<!doctype html>
<html lang="ja">
	<head>
		<%- include('parts/head', {title: 'プログラム一覧'}) %>
	</head>
	<body>
		<%- include('parts/navbar', {}) %>

		<div class="container p-2">

			<h1>プログラム一覧</h1>

			<% if (editable) { %>
				<div class="my-3">
					<a class="btn btn-primary" style="width: 130px;" href="/programs/edit">新規作成</a>
				</div>
			<% } %>

			<% for (let index in programs) { %>
				<% let program = programs[index] ; %>
				<div class="card my-1">
					<div class="card-body">
						<div class="row">
							<div class="col-sm">
								<a href="/programs/view?programId=<%= program.programId %>"><%= program.title.ja %></a>
							</div>
						</div>
					</div>
				</div>
			<% } %>
		</div>
	</body>
</html>