<!doctype html>
<html lang="ja">
	<head>
		<%- include('parts/head', {title: 'プログラム一覧'}) %>
	</head>
	<body>
		<%- include('parts/navbar', {}) %>

		<div class="container p-2">

			<h1>プログラム</h1>

			<% if (editable) { %>
				<div class="my-3">
					<a class="btn btn-primary" style="width: 130px;" href="/programs/edit?programId=<%= program.programId %>">編集</a>
				</div>
			<% } %>

			<div class="card my-1">
				<div class="card-body">
					<div class="row">
						<div class="col-sm">
							<%= program.title.ja %>
						</div>
					</div>
				</div>
			</div>

			<div class="my-3">
				<button type="button" class="btn btn-danger" style="width: 130px;" onclick="window.history.back(-1);return false;">戻る</button>
			</div>
		</div>
	</body>
</html>